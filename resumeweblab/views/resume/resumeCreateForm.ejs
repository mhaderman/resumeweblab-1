<%- include("../includes/header.ejs", {title:"Add New Resume"}); %>
<h1>Add New Resume</h1>
<form action="/Resume/insert" method="get">
    Resume Name: <input id="resume_id" name="resume_name" type="text" />
    <br />
    <br />
    School
    <br />
    <select id="school_name" name="school_id" multiple="multiple">
        <% school.forEach(function(i) { %>
        <option value="<%= (i).school_name %>"><%= (i).school_name%></option>
        <% }); %>
    </select>

    <!--Initialize the multiselect plugin: -->
    <script type="text/javascript">
        $(document).ready(function() {
            $('#school_name').multiselect();
        });
    </script>


    <br />
    <input type="submit" />

</form>
<%- include("../includes/footer.ejs"); %>
